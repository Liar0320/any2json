{
  "name": "nearhub",
  "version": "1.0.0",
  "description": "ANY2JSON library",
  "main": "index.js",
  "scripts": {
    "docs:dev": "pnpm -C docs dev",
    "docs:build": "concurrently \"pnpm run build\" \"pnpm -C docs build\"",
    "docs:preview": "pnpm -C docs preview",
    "scripts:dev": "pnpm -C scripts dev",
    "playground:dev": "pnpm -C playground dev",
    "dev": "concurrently \"npm run scripts:dev\" \"npm run docs:dev\"",
    "build": "pnpm -C scripts build",
    "test": "jest",
    "prepare": "husky install"
  },
  "keywords": [
    "ANY2JSON"
  ],
  "author": "Li chenghuan",
  "license": "ISC",
  "lint-staged": {
    "**/*.{ts,tsx,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Liar0320/any2json.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "buffer": "^5.5.0||^6.0.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "events": "^3.1.0",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "prettier": "^2.8.8",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "timers-browserify": "^2.0.11",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.3",
    "url": "^0.11.0"
  }
}
